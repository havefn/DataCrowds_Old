@model IEnumerable<DataCrowds.Models.Survey>

@{
    ViewBag.Title = "Reports";
}

<div class="page-header">
    <h2>@ViewBag.Title</h2>
</div>

<ul>
    @foreach (var survey in Model)
    {
        <li>
            <a href="@Url.Action("Details", new { id = survey.Id })">@survey.Name</a>
            <a href="@Url.Action("DownloadReports", new { id = survey.Id })">Downloads</a>
        </li>
    }
</ul>